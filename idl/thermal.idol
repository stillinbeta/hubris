// Thermal API

Interface(
    name: "Thermal",
    ops: {
        "set_mode_manual": (
            args: {
                "initial_pwm": "u8",
            },
            reply: Result(
                ok: "()",
                err: CLike("ThermalError"),
            ),
        ),
        "set_mode_auto": (
            reply: Result(
                ok: "()",
                err: CLike("ThermalError"),
            ),
        ),
        "get_mode": (
            reply: Result(
                ok: "ThermalMode",
                err: CLike("ThermalError"),
            ),
            encoding: Ssmarshal
        ),
        "get_auto_state": (
            reply: Result(
                ok: "ThermalAutoState",
                err: CLike("ThermalError"),
            ),
            encoding: Ssmarshal
        ),
        "set_fan_pwm": (
            args: {
                "index": "u8",
                "pwm": "u8",
            },
            reply: Result(
                ok: "()",
                err: CLike("ThermalError"),
            ),
        ),
        "disable_watchdog": (
            args: {},
            reply: Result(
                ok: "()",
                err: CLike("ThermalError"),
            ),
        ),
        "enable_watchdog": (
            args: {
                "timeout_s": "u8",
            },
            reply: Result(
                ok: "()",
                err: CLike("ThermalError"),
            ),
        ),
    },
)
